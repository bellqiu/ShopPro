<div class="login_left">
	<div class="box_title">Login to my account</div>
	<form id="FormLogin" method="post" action="/uc/login">
		<div>
			<p class="form_label">E-mail Address</p>
			<input type="text" name="LoginEmail" class="txt_input" value="">
			<div class="form_error red">asdasdasdasd</div>
			<p class="form_label">Password</p>
			<input type="password" name="LoginPwd" id="LoginPwd"
				class="txt_input">
			<p class="form_label">
				<input type="checkbox" name="rememberID" id="LoginRember" value="true" checked="checked">
				<span>&nbsp;&nbsp;Remember my E-mail Address for this
					computer</span>
			</p>
			<p class="form_label">
				<span class="cursor_point_and_underline">Forget Password?</span>
			</p>
			<input type="hidden" name="landing_page" value="${userView.requestPage!''}">
			<p class="form_label">
				<input name="" type="submit" class="button_01" value="Login">
			</p>
		</div>
	</form>

</div>

<div class="login_right">
		<div class="box_title">
			<p class="reg">New user registration</p>
		</div>
		<form >
			<p class="form_label">
				E-mail Address <span class="red">*</span>
			</p>
			<p>
				<input type="text" name="RegEmail" id="RegEmail" class="txt_input">
			</p>
			<p class="form_label gray">Please fill in a true and valid Email address,
				which will be a very important way for us to contact you.</p>
			<p class="form_label">
				Password <span  class="red">*</span>
			</p>
			<input type="password" name="RegPwd" id="RegPwd"  class="txt_input">
			<p class="gray form_label">Must have 6 to 30 characters and no spaces.</p>
			<span id="sp_pwd"></span>
			<p class="form_label">
				Re-type Password <span class="red">*</span>
			</p>
			<input type="password" name="RegPwdRe" id="RegPwdRe"  class="txt_input">
			<p>&nbsp;</p>
			<p>
				<input type="checkbox" name="checkbox" id="cbx_agree"
					checked="checked" class="cheo">&nbsp;&nbsp;I agree that I
				accept <a
					href="http://www.honeybuy.com/c/Terms-Of-Use"
					target="_blank" class="cursor_point_and_underline">Terms of
					Usage</a>
			</p>
			<p class="form_label">
				<input name="input" type="button" 
					class="button_01" value="Create a new account">
			</p>
		</form>
</div>