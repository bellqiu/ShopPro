<div class="main_box" id="main_box">
	<#include "include.hpl">
	<div class="tag-index-container">
	<#list (tagIndexs?keys) as key >
		<dl>
			<dt>${key}</dt>
			<#list (tagIndexs[key]) as keyword >
				<dd>
					<a
						href="">${keyword}</a>
				</dd>
			</#list>
		</dl>
		
		<#if ((key_index +1) % 4) == 0 >
			<p class="clearBoth"></p>
		</#if>
	</#list>
		<p class="clearBoth"></p>
	</div>
	<p class="clearBoth"></p>
</div>