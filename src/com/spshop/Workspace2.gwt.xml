<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="workspace2">
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<!-- This will give us shorter obfuscated CSS class names. We guarantee 
		that there will be no class names that will conflict with /M.+/ -->
	<set-configuration-property name="CssResource.obfuscationPrefix"
		value="M" />

	<entry-point class="com.spshop.admin.client.AdminWorkspace2"></entry-point>

	<source path='admin/client' />
	<source path='admin/shared' />
	<source path='admin/client/businessui' />
	<source path='model' />
	<source path='exception' />
	<replace-with class="com.spshop.admin.client.ClientFactoryImpl">
		<when-type-is class="com.spshop.admin.client.ClientFactory" />
	</replace-with>
</module>