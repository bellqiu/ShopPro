<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:InlineLabel text="Name:"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="name"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:InlineLabel text="Display Name:"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="displayName"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Button ui:field="button">Save</g:Button>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 