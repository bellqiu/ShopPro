<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:TabLayoutPanel barUnit="EM" barHeight="2.2" width="100em"
		height="50em" ui:field="tab">
		<g:tab>
			<g:header>
					Create/Edit Image
			</g:header>
			<g:FormPanel action="upload" method="post" ui:field="formPanel">
				<g:HTMLPanel>
					<div>
						<table>
							<tr>
								<td>Name:</td>
								<td>
									<g:TextBox ui:field="imageName"></g:TextBox>
								</td>
								<td></td>
							</tr>
							<tr>
								<td>Type:</td>
								<td>
									<g:ListBox ui:field="imageType"></g:ListBox>
								</td>
								<td></td>
							</tr>
							<tr>
								<td>Name:</td>
								<td>
									<g:FileUpload ui:field="imageFile"></g:FileUpload>
								</td>
								<td></td>
							</tr>
							<tr>
								<td>
									<g:SubmitButton ui:field="createImage">Create Image</g:SubmitButton>
								</td>
								<td></td>
							</tr>
						</table>
					</div>
				</g:HTMLPanel>
			</g:FormPanel>
		</g:tab>
	</g:TabLayoutPanel>
</ui:UiBinder> 