<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">

	<diskStore path="java.io.tmpdir"/>

	<defaultCache eternal="true" maxElementsInMemory="50"
		memoryStoreEvictionPolicy="LFU" />

	<cache name="global" eternal="false"
		timeToLiveSeconds="86400" maxElementsInMemory="5000"
		memoryStoreEvictionPolicy="LRU" overflowToDisk="true"
		maxElementsOnDisk="10000" />

	<cache name="others" eternal="false"
		timeToLiveSeconds="86400" maxElementsInMemory="1000"
		memoryStoreEvictionPolicy="LRU" overflowToDisk="true"
		maxElementsOnDisk="5000" />

</ehcache>
